library(shinydashboard)
options(shiny.maxRequestSize = 80*1024^2)
ui <- fluidPage(theme = shinytheme("flatly"),
shinyjs::useShinyjs(),
dashboardHeader(),
dashboardSidebar(
h3("Input", align = "center"),
br(),
fileInput('pdfs', 'Upload PDF file(s)', multiple = 'TRUE', accept = 'application/pdf'),
fileInput('longlists', 'Upload longlist file(s)', multiple = 'TRUE', accept = c('application/vnd.ms-excel', 'application/x-excel', 'application/x-msexcel', 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet')),
tableOutput("table.pdfs"),
tableOutput("table.longlists")
),
dashboardBody(
br(),
h3("Output", align = "center")
)
)
server <- function(input, output) {
output$table.pdfs <- renderTable({
files.pdf <- input$pdfs
table <- data.frame(matrix(ncol = 1, nrow = 0))
colnames(table) <- "No PDF file(s) uploaded"
if (is.null(files.pdf)){
return(table)
}
else{
if(length(files.pdf$name) == 1){
colnames(table) <- "Uploaded PDF file"
}
else{
colnames(table) <- "Uploaded PDF files"
}
for(pdf in 1:length(files.pdf$name)){
table[pdf,1] <- files.pdf$name[pdf]
}
return(table)
}
})
output$table.longlists <- renderTable({
files.longlist <- input$longlists
table <- data.frame(matrix(ncol = 1, nrow = 0))
colnames(table) <- "No Longlist file(s) uploaded"
if (is.null(files.longlist)){
return(table)
}
else{
if(length(files.longlist$name) == 1){
colnames(table) <- "Uploaded longlist file"
}
else{
colnames(table) <- "Uploaded longlist files"
}
for(longlist in 1:length(files.longlist$name)){
table[longlist,1] <- files.longlist$name[longlist]
}
return(table)
}
})
}
shinyApp(ui = ui, server = server)
